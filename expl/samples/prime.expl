int prime(int a){
    decl 
    int i,flag;
    enddecl
    begin
    i = 2;
    flag = 1;
    while(i<a) do
    if((a%i)==0) then
        flag =0;
    endif;
    i = i+1;
    endwhile;
    return flag;
    end
}
