alias currentPID R0;
currentPID = [SYSTEM_STATUS_TABLE+1];
[PROCESS_TABLE+currentPID*16+4] = TERMINATED;

alias counter R1;
counter = 1; // if except idle are terminated

while(counter<16) do
    if([PROCESS_TABLE + counter*16 + 4]!= TERMINATED) then
        call MOD_5;
    endif;
    counter = counter+1;
endwhile;

halt;



